                            <div class="input-verify">
                                <input type="text" class="form-control" id="name" required placeholder="Nome" name="name">
                                <div class="valid-feedback">
                                    Tudo certo por aqui!
                                </div>
                                <div class="invalid-feedback">
                                    Digite um nome ao cachorro.
                                </div>
                            </div>
                        </div>
                    </div>

